{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typebien</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typebien.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ typebien.libelle }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{% if typebien.statut %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ typebien.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ typebien.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ typebien.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ typebien.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ typebien.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_typebien_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_typebien_edit', { 'id': typebien.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

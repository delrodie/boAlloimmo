                                            <ul>
                                                <li>{{ similaire.zone }}</li>
                                                <li>{{ similaire.mode }}</li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
